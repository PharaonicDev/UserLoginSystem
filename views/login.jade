extends layout

block content
	.container
		h1 Login
		p please Register using the Form Below
		form(method='post', action='/users/register', enctype='multipart/form-data')
			.form-group
				label Username
				input.form-control(name='Username', type='text',placeholder='Enter Your Username')
			.form-group
				label password
				input.form-control(name='password', type='password')
			input.btn.btn-defult(name='submit', type='submit', value='Register')